<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PKHeXMAUI.TREditor"
             Title="TR Editor">
    <VerticalStackLayout>
        <CollectionView HeightRequest="500" WidthRequest="400" x:Name="trcollection" SelectionMode="Multiple" ></CollectionView>
        <Button x:Name="applytrs" Text="Apply" Clicked="applytrsandclose"></Button>
        <Button x:Name="applyalltrs" Text="Apply All" Clicked="applyalltrsandclose"/>
        <Button x:Name="removealltrs" Text="Remove All" Clicked="removealltrsandclose"/>
        <Button x:Name="close" Text="Close" Clicked="closer"/>
    </VerticalStackLayout>
</ContentPage>