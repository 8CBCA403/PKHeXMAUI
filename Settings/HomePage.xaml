<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PKHeXMAUI.HomePage"
             Title="Save Settings">
    <VerticalStackLayout>
        <Picker x:Name="SaveVersionPicker" Title="Current Save" SelectedIndexChanged="applynewsave"></Picker>
      
        <Button x:Name="savefileloader" Text="Open Save" Clicked="opensavefile"></Button>
        <Button Text="Inventory Editor" Clicked="OpenItems"/>
    </VerticalStackLayout>
</ContentPage>