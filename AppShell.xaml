<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="pk9reader.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:pk9reader">
    <TabBar>
        <Tab Title="Main">
            <ShellContent
        Title="Main"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
        </Tab>
        <Tab Title="Met">
            <ShellContent Title="Met"
                  ContentTemplate="{DataTemplate local:MetTab}"
                  Route="MetTab" />

        </Tab>
        <Tab Title="Stats">
            <ShellContent Title="Stats"></ShellContent></Tab>
        <Tab Title="Attacks">
            <ShellContent Title="Attacks"></ShellContent></Tab>
        <Tab Title="OT/Misc">
            <ShellContent Title="OT/MISC"></ShellContent></Tab>
    </TabBar>
</Shell>
